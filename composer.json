{
    "name": "matthewbdaly/laravel-internal-requests",
    "description": "Make internal requests with your Laravel app",
    "keywords": ["laravel","internal","requests"],
    "type": "library",
    "require-dev": {
        "mockery/mockery": "^1.0",
        "squizlabs/php_codesniffer": "^3.1",
        "psy/psysh": "^0.8.14",
        "php-coveralls/php-coveralls": "^2.1",
        "orchestra/testbench": "^3.7",
        "phpunit/phpunit": "^7.3"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Matthew Daly",
            "email": "matthewbdaly@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Matthewbdaly\\LaravelInternalRequests\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Matthewbdaly\\LaravelInternalRequests\\Providers\\InternalRequestServiceProvider"
            ]
        }
    }
}
